<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" schema="dbo"
	namespace="TAEM.Domain.GeoPolitica" assembly="TAEM.Domain">
	<class name="Provincia" table="Provincia">
		<id name="ID" type="System.Int32" column="IDProvincia" unsaved-value="0">
		</id>
		<property name="Nombre" type="System.String" length="100"/>
    <bag name="Comunas" cascade="all-delete-orphan">
      <key column="IDProvincia"/>
      <one-to-many class="Comuna"/>
    </bag>
    <many-to-one name="Region" column="IDRegion"/>
	</class>
</hibernate-mapping>
